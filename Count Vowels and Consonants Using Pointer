#include <stdio.h>
int main()
{
    char str[100], *p;
    int v=0,c=0;

    scanf("%s",str);
    p=str;

    while(*p)
    {
        if(*p=='a'||*p=='e'||*p=='i'||*p=='o'||*p=='u'||
           *p=='A'||*p=='E'||*p=='I'||*p=='O'||*p=='U')
            v++;
        else
            c++;
        p++;
    }
    printf("Vowels=%d Consonants=%d",v,c);
    return 0;
}
